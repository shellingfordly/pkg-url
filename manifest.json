{
  "name": "PKG-URL",
  "description": "Get url of the github project's package.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*"],
      "js": ["content-script.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/imgs/logo.png",
    "32": "/imgs/logo.png",
    "48": "/imgs/logo.png",
    "128": "/imgs/logo.png"
  }
}
